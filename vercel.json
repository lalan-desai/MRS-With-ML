{
    "builds": [
      {
        "src": "MRS_With_ML/wsgi.py",
        "use": "@vercel/python",
        "config": { 
          "maxLambdaSize": "15mb",
          "runtime": "python3.9" 
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "MRS_With_ML/wsgi.py"
      }
    ]
  }